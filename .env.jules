# Jules Agent Configuration
# Google Jules - Full Virtual Machine Agent (20GB capacity)
# Reference: https://github.com/gemini-cli-extensions/jules

# ===== JULES VM SETTINGS =====
JULES_ENABLED=true
JULES_VM_SIZE=20GB
JULES_VM_PATH=./jules_vm
JULES_VM_SNAPSHOT_INTERVAL=3600

# ===== JULES AGENT CAPABILITIES =====
JULES_ENABLE_CODE_EXECUTION=true
JULES_ENABLE_FILE_OPERATIONS=true
JULES_ENABLE_NETWORK_ACCESS=true
JULES_ENABLE_PACKAGE_INSTALL=true

# Supported languages (all from DEVELOPMENT_ENVIRONMENT.md)
JULES_PYTHON_VERSION=3.12.11
JULES_NODE_VERSION=22.16.0
JULES_JAVA_VERSION=21
JULES_GO_VERSION=1.24.3
JULES_RUST_VERSION=1.87.0

# ===== JULES DATA MANAGEMENT =====
# Store heavy data as skills to avoid token costs
JULES_SKILL_STORAGE_PATH=./skills/jules
JULES_MAX_SKILL_SIZE=5GB
JULES_AUTO_CACHE_DEPENDENCIES=true
JULES_CACHE_NODE_MODULES=true
JULES_CACHE_PYTHON_VENV=true

# ===== JULES PERFORMANCE =====
JULES_MAX_CONCURRENT_TASKS=5
JULES_TASK_TIMEOUT=1800
JULES_MEMORY_LIMIT=18GB
JULES_CPU_LIMIT=8

# ===== JULES INTEGRATION =====
JULES_GEMINI_INTEGRATION=true
JULES_CONDUCTOR_SYNC=true
JULES_SPEC_KIT_ENABLED=true
JULES_ANTIGRAVITY_IDE=true

# ===== JULES SECURITY =====
JULES_SANDBOX_MODE=true
JULES_ALLOWED_DOMAINS=github.com,googleapis.com,pypi.org,npmjs.com
JULES_NETWORK_RESTRICTIONS=true
JULES_FILE_ACCESS_RESTRICTIONS=./,/tmp

# ===== JULES LOGGING =====
JULES_LOG_LEVEL=INFO
JULES_LOG_PATH=./logs/jules.log
JULES_TRACE_EXECUTION=true
JULES_SAVE_SNAPSHOTS=true
