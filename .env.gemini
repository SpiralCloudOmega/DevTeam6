# Google Gemini CLI Configuration
# Token-efficient AI development with Google Gemini

# ===== GEMINI API SETTINGS =====
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash-exp
GEMINI_PRO_MODEL=gemini-pro
GEMINI_ULTRA_MODEL=gemini-ultra

# Alternative: Claude for interchangeability
CLAUDE_API_KEY=your_claude_api_key_here
CLAUDE_MODEL=claude-3-opus-20240229
USE_CLAUDE=false

# ===== GEMINI CLI SETTINGS =====
GEMINI_CLI_VERSION=latest
GEMINI_CLI_PATH=/usr/local/bin/gemini
GEMINI_MAX_TOKENS=8192
GEMINI_TEMPERATURE=0.7
GEMINI_TOP_P=0.95
GEMINI_TOP_K=40

# ===== TOKEN OPTIMIZATION =====
# Use skills for heavy data to reduce token costs
USE_SKILL_CACHE=true
SKILL_CACHE_PATH=./skills/cache
MAX_CONTEXT_TOKENS=100000
ENABLE_PROCEDURAL_MEMORY=true

# ===== STREAMING & PERFORMANCE =====
GEMINI_STREAM_RESPONSES=true
GEMINI_TIMEOUT=300
GEMINI_RETRY_ATTEMPTS=3
GEMINI_RATE_LIMIT_RPM=60

# ===== SAFETY SETTINGS =====
GEMINI_HARM_BLOCK_THRESHOLD=BLOCK_MEDIUM_AND_ABOVE
GEMINI_ENABLE_SAFETY_FILTERS=true

# ===== LOGGING =====
GEMINI_LOG_LEVEL=INFO
GEMINI_LOG_FILE=./logs/gemini.log
GEMINI_DEBUG_MODE=false
